@model COADto
<div class="page-header">
    <div class="page-title">
        <h4>@(ViewBag.NameOfForm)</h4>
        <h6>Delete @(ViewBag.NameOfForm)</h6>
    </div>
</div>

<div class="row">
     <form method="post">
        <input type="hidden" asp-for="Id" />
        <div class="col-lg-12 col-sm-12">
            <div class="card">
                <div class="card-body">
                    <div class="productdetails">
                        <ul class="product-bar">
                            <li>
                                <h4>Code</h4>
                                <h6>@Model.Code</h6>
                            </li>

                            <li>
                                <h4>AccountCode</h4>
                                <h6>@Model.AccountCode</h6>
                            </li>
                            <li>
                                <h4>AccountName</h4>
                                <h6>@Model.AccountName</h6>
                            </li>
                            <li>
                                <h4>ParentAccountName</h4>
                                <h6>@Model.ParentAccountName</h6>
                            </li>
                            <li>
                                <h4>CoaTranType</h4>
                                <h6>@Model.CoaTranType</h6>
                            </li>
                            <li>
                                <h4>BusinessCategoryName</h4>
                                <h6>@Model.BusinessCategoryName</h6>
                            </li>
                            <li>
                                <h4>COATypeName</h4>
                                <h6>@Model.COATypeName</h6>
                            </li>
                            <li>
                                <h4>LevelNo</h4>
                                <h6>@Model.LevelNo</h6>
                            </li>
                            <li>
                                <h4>Debit</h4>
                                <h6>@Model.Debit</h6>
                            </li>
                            <li>
                                <h4>Credit</h4>
                                <h6>@Model.Credit</h6>
                            </li>

                            <li>
                                <h4>Transable</h4>
                                <h6>@Model.Transable</h6>
                            </li>
                            <li>
                                <h4>Description</h4>
                                <h6>@Model.Description</h6>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div>
                <button class="btn btn-submit me-2">Submit</button>
                <a asp-action="Index" class="btn btn-cancel" id="bt">Cancel</a>
            </div>
        </div>
    </form>
</div>

